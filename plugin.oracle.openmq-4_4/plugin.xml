<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="com.mercatis.jms.connectionService">
      <connectionService
            class="com.mercatis.jms.openmq_4_4_0.OpenMqConnectionService"
            name="oracle_openmq">
         <property
               defaultValue="localhost"
               mandatory="true"
               name="Host"
               type="String">
         </property>
         <property
               defaultValue="7676"
               mandatory="true"
               name="Port"
               type="String">
         </property>
         <property
               defaultValue="admin"
               mandatory="false"
               name="Username"
               type="String">
         </property>
         <property
               defaultValue="admin"
               mandatory="false"
               name="Password"
               type="Password">
         </property>
      </connectionService>
   </extension>
      
</plugin>
